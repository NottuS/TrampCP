          			<var-value>situacaoOcupacao</var-value>
        		</var>
            </field>
        </form>  
        <form name="imprimirNotificacaoBemImovelForm">
        	<field property="codInstituicao" depends="required, integer">
            	<arg0 key="relatorios.instituicao"/>
            </field>
            <field property="tsNotificacaoDe" depends="required, date, dataServidor">
            	<msg name="validwhen" key="imprimirNotificacaoBemImovelForm.tsNotificacaoAteMenortsNotificacaoDe"/>
            	<arg0 key="imprimirNotificacaoBemImovelForm.tsNotificacaoDe"/>
               	<var>
			         <var-name>datePatternStrict</var-name> 
			         <var-value>dd/MM/yyyy</var-value> 
			    </var> 
			    <var>
   					<var-name>test</var-name>
          			<var-value>( tsNotificacaoAte > *this*)</var-value>
        		</var>
                                
            </field> 
            <field property="tsNotificacaoAte" depends="required, date, dataServidor">
            	<msg name="validwhen" key="imprimirNotificacaoBemImovelForm.tsNotificacaoAteMenortsNotificacaoDe"/>
            	<arg0 key="imprimirNotificacaoBemImovelForm.tsNotificacaoAte"/>
            	<var>
			         <var-name>datePatternStrict</var-name> 
			         <var-value>dd/MM/yyyy</var-value> 
			    </var> 
			    <var>
   					<var-name>test</var-name>
          			<var-value>(*this* > tsNotificacaoDe)</var-value>
        		</var>
                                
            </field> 
        </form> 
  
