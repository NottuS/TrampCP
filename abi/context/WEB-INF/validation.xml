		 -->

		<!-- Bem Imovel -->
        <form name="bemImovelForm">
            <field property="nrBemImovel" depends="required, maxlength, integer">
				<msg name="integer" key="bemImovelForm.nrBemImovel.integer"/>
				<arg0 key="bemImovelForm.nrBemImovel"/>
				<arg1 name="maxlength" key="${var:maxlength}" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>12</var-value>
				</var>
			</field>
                
            <field property="orgao" depends="validwhen">
            	<arg0 key="bemImovelForm.orgao"/>
            	<var>
   					<var-name>test</var-name>
          			<var-value>((administracao == 1) or (*this* != null))</var-value>
        		</var>
            </field>
         
	        <field property="classificacaoBemImovel" depends="required">
	            	<arg0 key="bemImovelForm.classificacaoBemImovel"/>
	        </field>
	        
	        <field property="situacaoLegalCartorial" depends="required">
	            	<arg0 key="bemImovelForm.situacaoLegalCartorial"/>
	        </field>
	        <field property="numeroProcessoSpi" depends="maxlength">
	           	<arg0 key="bemImovelForm.numeroProcessoSpi"/>
	           	<arg1 name="maxlength" key="${var:maxlength}" resource="false"/>
	           	<var>
	                   <var-name>maxlength</var-name>
	                   <var-value>15</var-value>
	            </var>                   
	        </field> 
	        <field property="uf" depends="required">
	            	<arg0 key="bemImovelForm.uf"/>
	        </field>
	        <field property="codMunicipio" depends="required">
	            	<arg0 key="bemImovelForm.municipio"/>
	        </field>
	        <field property="areaTerreno" depends="required">
	            	<arg0 key="bemImovelForm.areaTerreno"/>
	        </field>
	        <field property="bairroDistrito" depends="maxlength">
	           	<arg0 key="bemImovelForm.bairroDistrito"/>
	           	<arg1 name="maxlength" key="${var:maxlength}" resource="false"/>
	           	<var>
	                   <var-name>maxlength</var-name>
	                   <var-value>60</var-value>
	            </var>                   
	        </field> 
	       
