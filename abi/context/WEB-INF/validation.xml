                    <var-name>maxlength</var-name>
                    <var-value>15</var-value>
                </var>      
            	<var>
   					<var-name>situacaoOcupacao</var-name>
          			<var-value>situacaoOcupacao</var-value>
        		</var>
            </field>
            <field property="prazoNotificacao" depends="validaOcupacao, maxlength">
            	<arg0 key="ocupacaoForm.prazoNotificacao"/>
            	<arg1 name="maxlength" key="${var:maxlength}" resource="false"/>
            	<var>
                    <var-name>maxlength</var-name>
                    <var-value>15</var-value>
                </var>      
            	<var>
   					<var-name>situacaoOcupacao</var-name>
          			<var-value>situacaoOcupacao</var-value>
        		</var>
            </field>
            <field property="protocoloNotificacaoSpi" depends="validaOcupacao, integer">
            	<arg0 key="ocupacaoForm.protocoloNotificacaoSpi"/>
