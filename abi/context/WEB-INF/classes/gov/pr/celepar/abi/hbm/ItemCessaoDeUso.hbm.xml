<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="gov.pr.celepar.abi.pojo.ItemCessaoDeUso" schema="public" table="tb_item_cessao_de_uso">
  <id name="codItemCessaoDeUso" type="java.lang.Integer">
   <meta attribute="use-in-equals">true</meta>
   <column name="cod_item_cessao_de_uso"/>
   <generator class="sequence">
    <param name="sequence">tb_item_cessao_de_uso_cod_item_cessao_de_uso_seq</param>
   </generator>
  </id>
  <many-to-one class="gov.pr.celepar.abi.pojo.CessaoDeUso" fetch="select" name="cessaoDeUso">
   <column name="cod_cessao_de_uso" not-null="true"/>
  </many-to-one>
  <many-to-one class="gov.pr.celepar.abi.pojo.Edificacao" fetch="select" name="edificacao">
   <column name="cod_edificacao"/>
  </many-to-one>
  <property name="indTerreno" type="java.lang.Boolean">
   <column name="ind_terreno" not-null="false"/>
  </property>
  <property name="caracteristica" type="java.lang.String">
   <column length="3000" name="caracteristica" not-null="true"/>
  </property>
  <property name="utilizacao" type="java.lang.String">
   <column length="3000" name="utilizacao" not-null="true"/>
  </property>
  <property name="observacao" type="java.lang.String">
   <column length="3000" name="observacao" not-null="false"/>
  </property>
  <property name="areaMetroQuadrado" type="big_decimal">
   <column name="area_metro_quadrado" not-null="false"/>
  </property>
  <property name="areaPercentual" type="big_decimal">
   <column name="area_percentual" not-null="false"/>
  </property>
  <property name="tipo" type="java.lang.Integer">
   <column name="tipo" not-null="true"/>
  </property>
  <property name="situacaoDominial" type="java.lang.String">
   <column length="3000" name="situacao_dominial" not-null="true"/>
  </property>
 </class>
</hibernate-mapping>
