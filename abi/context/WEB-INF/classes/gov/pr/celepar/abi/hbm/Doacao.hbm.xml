<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="gov.pr.celepar.abi.pojo.Doacao" schema="public" table="tb_doacao">
  <id name="codDoacao" type="java.lang.Integer">
   <meta attribute="use-in-equals">true</meta>
   <column name="cod_doacao"/>
   <generator class="sequence">
    <param name="sequence">tb_doacao_cod_doacao_seq</param>
   </generator>
  </id>
  <many-to-one class="gov.pr.celepar.abi.pojo.BemImovel" fetch="select" name="bemImovel">
   <column name="cod_bem_imovel" not-null="true"/>
  </many-to-one>
  <many-to-one class="gov.pr.celepar.abi.pojo.Orgao" fetch="select" name="orgaoProprietario">
   <column name="cod_orgao_proprietario"/>
  </many-to-one>
  <many-to-one class="gov.pr.celepar.abi.pojo.Orgao" fetch="select" name="orgaoResponsavel">
   <column name="cod_orgao_responsavel" not-null="true"/>
  </many-to-one>
  <many-to-one class="gov.pr.celepar.abi.pojo.StatusTermo" fetch="select" name="statusTermo">
   <column name="cod_status_termo" not-null="true"/>
  </many-to-one>
  <many-to-one class="gov.pr.celepar.abi.pojo.LeiBemImovel" fetch="select" name="leiBemImovel" lazy="false">
   <column name="cod_lei_bem_imovel"/>
  </many-to-one>
  <many-to-one class="gov.pr.celepar.abi.pojo.Instituicao" fetch="select" name="instituicao" lazy="false">
   <column name="cod_instituicao" not-null="true"/>
  </many-to-one>
  <property name="administracao" type="java.lang.Integer">
   <column name="administracao" not-null="true"/>
  </property>
  <property name="protocolo" type="java.lang.String">
   <column length="30" name="protocolo" not-null="false"/>
  </property>
  <property name="motivoRevogacao" type="java.lang.String">
   <column length="3000" name="motivo_revogacao" not-null="false"/>
  </property>
  <property name="nrOficio" type="java.lang.Integer">
   <column name="nr_oficio" not-null="false"/>
  </property>
  <property name="dataRegistro" type="date">
   <column name="data_registro" not-null="false"/>
  </property>
  <property name="dtInicioVigencia" type="date">
   <column name="dt_inicio_vigencia" not-null="false"/>
  </property>
  <property name="dtFimVigencia" type="date">
   <column name="dt_fim_vigencia" not-null="false"/>
  </property>
  <property name="nrProjetoLei" type="java.lang.Integer">
   <column name="nr_projeto_lei" not-null="false"/>
  </property>
  <property name="tsInclusao" type="timestamp">
   <column length="8" name="ts_inclusao" not-null="true"/>
  </property>
  <property name="tsAtualizacao" type="timestamp">
   <column length="8" name="ts_atualizacao"/>
  </property>
  <property name="tsExclusao" type="timestamp">
   <column length="8" name="ts_exclusao"/>
  </property>
  <property name="cpfResponsavelInclusao" type="java.lang.String">
   <column name="cpf_responsavel_inclusao" not-null="true"/>
  </property>
  <property name="cpfResponsavelAlteracao" type="java.lang.String">
   <column name="cpf_responsavel_alteracao"/>
  </property>
  <property name="cpfResponsavelExclusao" type="java.lang.String">
   <column name="cpf_responsavel_exclusao"/>
  </property>
  <set inverse="true" name="listaAssinaturaDoacao" sort="unsorted">
   <key>
    <column name="cod_doacao" not-null="true"/>
   </key>
   <one-to-many class="gov.pr.celepar.abi.pojo.AssinaturaDoacao"/>
  </set>
  <set inverse="true" name="listaItemDoacao" sort="unsorted">
   <key>
    <column name="cod_doacao" not-null="true"/>
   </key>
   <one-to-many class="gov.pr.celepar.abi.pojo.ItemDoacao"/>
  </set>
 </class>
</hibernate-mapping>
