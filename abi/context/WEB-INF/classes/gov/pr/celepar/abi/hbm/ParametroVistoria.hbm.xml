<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="gov.pr.celepar.abi.pojo.ParametroVistoria" schema="public" table="tb_parametro_vistoria">
  <id name="codParametroVistoria" type="java.lang.Integer">
      <meta attribute="use-in-equals">true</meta>
      <column name="cod_parametro_vistoria"/>
      <generator class="sequence">
      	<param name="sequence">tb_parametro_vistoria_cod_parametro_vistoria_seq</param>
      </generator>
    </id>
  <property generated="never" lazy="false" name="indTipoParametro" type="java.lang.Integer">
   <column name="ind_tipo_parametro"/>
  </property>
  <property generated="never" lazy="false" name="indAtivo" type="java.lang.Boolean">
   <column name="ind_ativo"/>
  </property>
  <property generated="never" lazy="false" name="ordemApresentacao" type="java.lang.Integer">
   <column name="ordem_apresentacao"/>
  </property>
  <property generated="never" lazy="false" name="descricao" type="java.lang.String">
   <column name="descricao"/>
  </property>
  <property generated="never" lazy="false" name="tsInclusao" type="timestamp">
   <column length="8" name="ts_inclusao" not-null="true"/>
  </property>
  <property generated="never" lazy="false" name="tsAtualizacao" type="timestamp">
   <column length="8" name="ts_atualizacao"/>
  </property>
  <property generated="never" lazy="false" name="cpfResponsavel" type="java.lang.String">
   <column name="cpf_responsavel" not-null="true"/>
  </property>
  <many-to-one class="gov.pr.celepar.abi.pojo.Instituicao" fetch="select" name="instituicao" lazy="false">
      <column name="cod_instituicao" not-null="true"/>
  </many-to-one>
  <set inverse="true" name="listaParametroVistoriaDominio" lazy="false">
   <key>
    <column name="cod_parametro_vistoria" not-null="true"/>
   </key>
   <one-to-many class="gov.pr.celepar.abi.pojo.ParametroVistoriaDominio"/>
  </set>
  
  <set inverse="true" name="listaParametroVistoriaDenominacaoImovel" lazy="false"  cascade="all">
      <key>
        <column name="cod_parametro_vistoria" not-null="true"/>
      </key>
      <one-to-many class="gov.pr.celepar.abi.pojo.ParametroVistoriaDenominacaoImovel"/>
  </set>   
  <set inverse="true" name="listaItemVistoria" cascade="all">
    <key>
      <column name="cod_parametro_vistoria" not-null="true"/>
    </key>
    <one-to-many class="gov.pr.celepar.abi.pojo.ItemVistoria"/>
  </set>  

 </class>
</hibernate-mapping>
