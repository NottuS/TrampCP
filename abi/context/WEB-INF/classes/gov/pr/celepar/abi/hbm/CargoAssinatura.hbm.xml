<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="gov.pr.celepar.abi.pojo.CargoAssinatura" schema="public" table="tb_cargo_assinatura">
  <id name="codCargoAssinatura" type="java.lang.Integer">
   <meta attribute="use-in-equals">true</meta>
   <column name="cod_cargo_assinatura"/>
   <generator class="sequence">
    <param name="sequence">tb_cargo_assinatura_cod_cargo_assinatura_seq</param>
   </generator>
  </id>
  <property name="descricao" type="java.lang.String">
   <column length="100" name="descricao"/>
  </property>
  <many-to-one class="gov.pr.celepar.abi.pojo.Instituicao"
      fetch="select" name="instituicao" lazy="false">
      <column name="cod_instituicao" not-null="true"/>
  </many-to-one>
  <set inverse="true" name="listaAssinatura" sort="unsorted">
   <key>
    <column name="cod_cargo_assinatura" not-null="true"/>
   </key>
   <one-to-many class="gov.pr.celepar.abi.pojo.Assinatura"/>
  </set>
 </class>
</hibernate-mapping>
