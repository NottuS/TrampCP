<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="gov.pr.celepar.abi.pojo.AssinaturaDocTransferencia" schema="public" table="tb_assinatura_doc_transferencia">
  <id name="codAssinaturaDocTransferencia" type="java.lang.Integer">
   <meta attribute="use-in-equals">true</meta>
   <column name="cod_assinatura_doc_transferencia"/>
   <generator class="sequence">
    <param name="sequence">tb_assinatura_doc_transferenc_cod_assinatura_doc_transferen_seq</param>
   </generator>
  </id>
  <many-to-one class="gov.pr.celepar.abi.pojo.Transferencia" fetch="select" name="transferencia">
   <column name="cod_transferencia" not-null="true"/>
  </many-to-one>
  <many-to-one class="gov.pr.celepar.abi.pojo.Assinatura" fetch="select" name="assinatura">
   <column name="cod_assinatura" not-null="true"/>
  </many-to-one>
 </class>
</hibernate-mapping>
