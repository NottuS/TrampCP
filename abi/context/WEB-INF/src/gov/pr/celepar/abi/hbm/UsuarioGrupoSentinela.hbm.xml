<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="gov.pr.celepar.abi.pojo.UsuarioGrupoSentinela" schema="public" table="tb_usuario_grupo_sentinela">
	<composite-id>
		<key-many-to-one name="usuario" 
        				 class="gov.pr.celepar.abi.pojo.Usuario" 
        				 column="cod_usuario" lazy="false"/>
		<key-many-to-one name="grupoSentinela" 
        				 class="gov.pr.celepar.abi.pojo.GrupoSentinela" 
        				 column="cod_grupo_sentinela" lazy="false"/>
	</composite-id>
  </class>
</hibernate-mapping>