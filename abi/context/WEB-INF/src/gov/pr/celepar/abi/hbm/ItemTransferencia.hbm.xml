<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="gov.pr.celepar.abi.pojo.ItemTransferencia" schema="public" table="tb_item_transferencia">
  <id name="codItemTransferencia" type="java.lang.Integer">
   <meta attribute="use-in-equals">true</meta>
   <column name="cod_item_transferencia"/>
   <generator class="sequence">
    <param name="sequence">tb_item_transferencia_cod_item_transferencia_seq</param>
   </generator>
  </id>
  <many-to-one class="gov.pr.celepar.abi.pojo.Transferencia" fetch="select" name="transferencia">
   <column name="cod_transferencia" not-null="true"/>
  </many-to-one>
  <many-to-one class="gov.pr.celepar.abi.pojo.Edificacao" fetch="select" name="edificacao">
   <column name="cod_edificacao" not-null="false"/>
  </many-to-one>
  <property name="caracteristica" type="java.lang.String">
   <column length="3000" name="caracteristica" not-null="true"/>
  </property>
  <property name="situacaoDominial" type="java.lang.String">
   <column length="3000" name="situacao_dominial" not-null="true"/>
  </property>
  <property name="utilizacao" type="java.lang.String">
   <column length="3000" name="utilizacao" not-null="true"/>
  </property>
  <property name="observacao" type="java.lang.String">
   <column length="3000" name="observacao" not-null="false"/>
  </property>
  <property name="transferenciaMetros" type="big_decimal">
   <column name="transferencia_metros" not-null="false"/>
  </property>
  <property name="transferenciaPercentual" type="big_decimal">
   <column name="transferencia_percentual" not-null="false"/>
  </property>
  <property name="somenteTerreno" type="java.lang.Boolean">
   <column name="somente_terreno" not-null="false"/>
  </property>
  <property name="tipo" type="java.lang.Integer">
   <column name="tipo" not-null="true"/>
  </property>
 </class>
</hibernate-mapping>
