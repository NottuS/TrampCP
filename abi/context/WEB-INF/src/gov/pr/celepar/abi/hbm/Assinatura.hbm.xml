<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="gov.pr.celepar.abi.pojo.Assinatura" schema="public" table="tb_assinatura">
  <id name="codAssinatura" type="java.lang.Integer">
   <meta attribute="use-in-equals">true</meta>
   <column name="cod_assinatura"/>
   <generator class="sequence">
    <param name="sequence">tb_assinatura_cod_assinatura_seq</param>
   </generator>
  </id>
  <many-to-one class="gov.pr.celepar.abi.pojo.CargoAssinatura" fetch="select" name="cargoAssinatura">
   <column name="cod_cargo_assinatura" not-null="true"/>
  </many-to-one>
  <many-to-one class="gov.pr.celepar.abi.pojo.Orgao" fetch="select" name="orgao">
   <column name="cod_orgao" not-null="true"/>
  </many-to-one>
  <many-to-one class="gov.pr.celepar.abi.pojo.Instituicao" fetch="select" name="instituicao">
   <column name="cod_instituicao" not-null="true"/>
  </many-to-one>
  <property name="cpf" type="java.lang.String">
   <column length="11" name="cpf"/>
  </property>
  <property name="nome" type="java.lang.String">
   <column length="150" name="nome" not-null="true"/>
  </property>
  <property name="indAtivo" type="java.lang.Boolean">
   <column name="ind_ativo" not-null="true"/>
  </property>
  <property name="indResponsavelMaximo" type="java.lang.Boolean">
   <column name="ind_responsavel_maximo" not-null="true"/>
  </property>
  <property name="administracao" type="java.lang.Integer">
   <column name="administracao" not-null="true"/>
  </property>
  <property name="tsInclusao" type="timestamp">
   <column length="8" name="ts_inclusao" not-null="true"/>
  </property>
  <property name="tsAtualizacao" type="timestamp">
   <column length="8" name="ts_atualizacao"/>
  </property>
  <property name="tsExclusao" type="timestamp">
   <column length="8" name="ts_exclusao"/>
  </property>
  <property name="cpfResponsavelInclusao" type="java.lang.String">
   <column name="cpf_responsavel_inclusao" not-null="true"/>
  </property>
  <property name="cpfResponsavelAlteracao" type="java.lang.String">
   <column name="cpf_responsavel_alteracao"/>
  </property>
  <property name="cpfResponsavelExclusao" type="java.lang.String">
   <column name="cpf_responsavel_exclusao"/>
  </property>
  <set inverse="true" name="listaAssinaturaDoacao" sort="unsorted">
   <key>
    <column name="cod_assinatura" not-null="true"/>
   </key>
   <one-to-many class="gov.pr.celepar.abi.pojo.AssinaturaDoacao"/>
  </set>
  <set inverse="true" name="listaAssinaturaTransferencia" sort="unsorted">
   <key>
    <column name="cod_assinatura" not-null="true"/>
   </key>
   <one-to-many class="gov.pr.celepar.abi.pojo.AssinaturaTransferencia"/>
  </set>
  <set inverse="true" name="listaAssinaturaDocTransferencia" sort="unsorted">
   <key>
    <column name="cod_assinatura" not-null="true"/>
   </key>
   <one-to-many class="gov.pr.celepar.abi.pojo.AssinaturaDocTransferencia"/>
  </set>
 </class>
</hibernate-mapping>
