<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="gov.pr.celepar.abi.pojo.ParametroAgenda" schema="public" table="tb_parametro_agenda">
  <id name="codParametroAgenda" type="java.lang.Integer">
   <meta attribute="use-in-equals">true</meta>
   <column name="cod_parametro_agenda"/>
   <generator class="sequence">
    <param name="sequence">tb_parametro_agenda_cod_parametro_agenda_seq</param>
   </generator>
  </id>
  <property generated="never" lazy="false"
   name="numeroDiasVencimentoNotificacao" type="java.lang.Integer">
   <column name="numero_dias_vencimento_notificacao"/>
  </property>
  <property generated="never" lazy="false"
   name="numeroDiasVencimentoCessaoDeUso" type="java.lang.Integer">
   <column name="numero_dias_vencimento_cessao_de_uso"/>
  </property>
  <property generated="never" lazy="false"
   name="numeroDiasVencimentoDoacao" type="java.lang.Integer">
   <column name="numero_dias_vencimento_doacao"/>
  </property>
  <property generated="never" lazy="false"
   name="numeroDiasVencimentoVistoria" type="java.lang.Integer">
   <column name="numero_dias_vencimento_vistoria"/>
  </property>
  <property generated="never" lazy="false" name="tsInclusao" type="timestamp">
   <column length="8" name="ts_inclusao" not-null="true"/>
  </property>
  <property generated="never" lazy="false" name="tsAtualizacao" type="timestamp">
   <column length="8" name="ts_atualizacao"/>
  </property>
  <property generated="never" lazy="false" name="cpfResponsavel" type="java.lang.String">
   <column name="cpf_responsavel" not-null="true"/>
  </property>
  <property generated="never" lazy="false" name="tempoCessao" type="java.lang.Integer">
   <column name="tempo_cessao"/>
  </property>
  <many-to-one class="gov.pr.celepar.abi.pojo.Instituicao" fetch="select" name="instituicao" lazy="false">
      <column name="cod_instituicao" not-null="true"/>
    </many-to-one>
  <set inverse="true" name="listaParametroAgendaEmail" lazy="false">
   <key>
    <column name="cod_parametro_agenda" not-null="true"/>
   </key>
   <one-to-many class="gov.pr.celepar.abi.pojo.ParametroAgendaEmail"/>
  </set>
 </class>
</hibernate-mapping>
