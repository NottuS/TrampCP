<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    								"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="gov.pr.celepar.framework.pojo.Endereco" table="tb_endereco">
    <id column="id_endereco" name="idEndereco" type="java.lang.Integer">
      <generator class="sequence">
      		<param name="sequence">tb_endereco_id_endereco_seq</param>
      </generator>
    </id>
    <property column="logradouro_endereco" length="128" name="logradouroEndereco" not-null="true" type="java.lang.String" />
    <property column="numero_endereco" length="16" name="numeroEndereco" not-null="true" type="java.lang.String" />
    <property column="complemento_endereco" length="16" name="complementoEndereco" type="java.lang.String" />
    <property column="bairro_endereco" length="64" name="bairroEndereco" not-null="true" type="java.lang.String" />
    <property column="cidade_endereco" length="128" name="cidadeEndereco" not-null="true" type="java.lang.String" />
    <property column="uf_endereco" length="2" name="ufEndereco" not-null="true" type="java.lang.String" />
    <property column="cep_endereco" length="8" name="cepEndereco" not-null="true" type="java.lang.Integer" />
    <property column="id_localidade" length="8" name="idLocalidade" not-null="true" type="java.lang.Integer" />
    <!-- Associations -->
    <!-- bi-directional one-to-many association to TbAluno -->
    <set cascade="none" inverse="true" lazy="true" name="alunos">
      <key>
        <column name="endereco"/>
      </key>
      <one-to-many class="gov.pr.celepar.framework.pojo.Aluno"/>
    </set>
    <!-- bi-directional one-to-many association to TbEscola -->
    <set cascade="none" inverse="true" lazy="true" name="escolas">
      <key>
        <column name="endereco"/>
      </key>
      <one-to-many class="gov.pr.celepar.framework.pojo.Escola"/>
    </set>
  </class>
</hibernate-mapping>